{
  "name": "husk",
  "description": "Command execution as transform streams.",
  "version": "0.1.4",
  "author": "muji <noop@xpm.io>",
  "repository": {
    "type": "git",
    "url": "git://github.com/freeformsystems/husk.git"
  },
  "main": "lib/index.js",
  "dependencies": {
    "zephyr": "~1.2.5"
  },
  "devDependencies": {
    "async": "~0.9.0",
    "chai": "~2.2.0",
    "istanbul": "~0.3.13",
    "mocha": "~2.2.4",
    "stream-lines": "~1.0.3"
  },
  "engine": [
    "node >= 0.10.11"
  ],
  "keywords": [
    "stream",
    "transform",
    "lines",
    "parse",
    "read",
    "shell",
    "exec",
    "json"
  ],
  "scripts": {
    "docs": "npm run readme",
    "readme": "mdp --force -v",
    "ebin": "./sbin/ebin",
    "test": "NODE_ENV=test mocha test/spec",
    "cover": "NODE_ENV=test istanbul cover --root ./lib _mocha -- test/spec"
  },
  "mdp": {
    "title": "Husk",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/freeformsystems/husk",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md"
        ]
      },
      {
        "title": "Example",
        "bin": "sbin/example"
      },
      {
        "inc": [
          "developer.md",
          "license.md"
        ]
      }
    ]
  }
}
