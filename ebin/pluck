#!/usr/bin/env node

var husk = require('..').core().fs()
  .plugin([
    require('husk-buffer'),
    require('husk-parse'),
    require('husk-filter'),
    require('husk-pluck'),
    require('husk-stringify'),
  ]);

husk('package.json')
  .read()
  .buffer()
  .parse()
  .pluck(function(){return this.dependencies})
  .stringify({indent: 2})
  .print()
  .run();
